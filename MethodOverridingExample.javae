class Animals {
    void sound() {
        System.out.println("Animal makes a sound");
    }
}
class Cat extends Animals {
    @Override
    void sound() {
        System.out.println("Cat meows");
    }
}
public class mov{
    public static void main(String[] args) {
        Animals animal= new Animals();
        animal.sound();
        Animals animals= new Cat();
        animals.sound();
    }
}
